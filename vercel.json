{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/auth$", "dest": "/api/auth.ts" },
    { "src": "/api/auth/(.*)", "dest": "/api/auth.ts" },
    { "src": "/api/admin$", "dest": "/api/admin/[...path].ts" },
    { "src": "/api/admin/(.*)", "dest": "/api/admin/[...path].ts" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
